{
  "ReverseProxy": {
    "Routes": {
      "add-main-article-content": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/{articleId}/contents",
          "Methods": [ "POST" ]
        },
        "AuthorizationPolicy": "AMW",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "create-articles": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/",
          "Methods": [ "POST" ]
        },
        "AuthorizationPolicy": "AMW",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "edit-article": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/{articleId}",
          "Methods": [ "PATCH" ]
        },
        "AuthorizationPolicy": "AMW",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-article-characteristics": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/characteristics/{articleId}",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-article-content": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/{articleId}/contents",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-article-crosses": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/{articleId}/crosses/",
