        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-article-images": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/imgs",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-article-pairs": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/{articleId}/pairs",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-articles": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "make-linkage-between-articles": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/crosses",
          "Methods": [ "POST" ]
        },
        "AuthorizationPolicy": "AMW",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "map-imgs-to-article": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/{articleId}/imgs/",
          "Methods": [ "POST" ]
        },
        "AuthorizationPolicy": "AMW",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "remove-article-content": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/{articleId}/contents/{insideArticleId}",
          "Methods": [ "DELETE" ]
        },
        "AuthorizationPolicy": "AMW",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "set-article-indicator": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/{articleId}/indicator",
