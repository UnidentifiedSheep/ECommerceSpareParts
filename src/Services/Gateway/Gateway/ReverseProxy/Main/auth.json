{
  "ReverseProxy": {
    "Routes": {
      "confirm-mail": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/auth/verify/mail",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "login": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/auth/login",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "refresh-token": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/auth/refresh",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "register": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/auth/register/",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      }
    }
  }
}