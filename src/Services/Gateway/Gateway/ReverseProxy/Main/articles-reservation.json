{
  "ReverseProxy": {
    "Routes": {
      "create-article-reservation": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/reservations",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "delete-article-reservation": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/reservations/{reservationId}",
          "Methods": [ "DELETE" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "edit-article-reservation": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/reservations/{reservationId}",
          "Methods": [ "PUT" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-article-reservations": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/articles/reservations",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      }
    }
  }
}
