{
  "ReverseProxy": {
    "Routes": {
      "create-sale": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/sales/",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "delete-sale": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/sales/{saleId}",
          "Methods": [ "DELETE" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "edit-sale": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/sales/{saleId}",
          "Methods": [ "PUT" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-sale-content": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/sales/{id}/content",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-sales": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/sales/",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      }
    }
  }
}