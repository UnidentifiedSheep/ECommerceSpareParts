{
  "ReverseProxy": {
    "Routes": {
      "add-vehicle-to-garage": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/vehicles",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "add-vehicle-to-garage-user": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/me/vehicles",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "change-discount-for-user": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/discount",
          "Methods": [ "PATCH" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "create-mail-for-user": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/mail/corporate",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "create-users": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-users": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "add-permission-to-user": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/permissions",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-user-full-info": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/info",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-user-discount": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/discount",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "add-storage-to-user": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/storages/{storageName}",
          "Methods": [ "POST" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-user-storages": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/storages",
          "Methods": [ "GET" ]
        },
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      }
    }
  }
}