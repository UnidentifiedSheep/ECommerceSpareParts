{
  "ReverseProxy": {
    "Routes": {
      "add-vehicle-to-garage": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/vehicles/",
          "Methods": [ "POST" ]
        },
        "AuthorizationPolicy": "AMW",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "add-vehicle-to-garage-user": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/me/vehicles/",
          "Methods": [ "POST" ]
        },
        "AuthorizationPolicy": "MEMBER",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "change-discount-for-user": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/discount/",
          "Methods": [ "PATCH" ]
        },
        "AuthorizationPolicy": "AM",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "create-mail-for-user": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/{userId}/mail/corporate",
          "Methods": [ "POST" ]
        },
        "AuthorizationPolicy": "AM",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "create-users": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/",
          "Methods": [ "POST" ]
        },
        "AuthorizationPolicy": "AM",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      },
      "get-users": {
        "ClusterId": "main-cluster",
        "Match": {
          "Path": "/main/users/",
          "Methods": [ "GET" ]
        },
        "AuthorizationPolicy": "AMW",
        "Transforms": [
          { "PathRemovePrefix": "/main" }
        ]
      }
    }
  }
}